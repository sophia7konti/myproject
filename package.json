{
  "name": "myfinalproject",
  "version": "1.0.0",
  "description": "Μια απλή εφαρμογή ημερολογίου όπου ο χρήστης μπορεί να κάνει login και να αποθηκεύει τις εγγραφές του.",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "node backend/server.js",
    "start-frontend": "cross-env PORT=3003 npm start --prefix frontend",
    "install-all": "npm install --prefix backend && npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "backend": "npm start --prefix backend",
    "build-frontend": "npm run build --prefix frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "concurrently": "^9.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
